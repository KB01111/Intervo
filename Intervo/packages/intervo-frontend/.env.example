# Frontend Environment Variables
# Rename this file to .env.local and fill in your actual values.
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=a_super_secret_key_for_nextauth

# Frontend API URLs
NEXT_PUBLIC_API_URL_PRODUCTION=https://api.intervo.ai
NEXT_PUBLIC_API_URL_DEVELOPMENT=http://localhost:4000

# You can add backend variables here as well, for example:
# MONGO_URI=your_mongodb_connection_string
# STRIPE_SECRET_KEY=your_stripe_secret_key 